<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="454.0" prefWidth="287.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.groupController">
   <children>
      <TextField fx:id="searchField" layoutX="30.0" layoutY="200.0" onAction="#handleSearch" />
      <Button fx:id="userAdd" layoutX="32.0" layoutY="169.0" mnemonicParsing="false" onAction="#buttonHandle" text="Add User" />
      <ListView fx:id="resultsView" layoutX="30.0" layoutY="233.0" prefHeight="195.0" prefWidth="149.0" />
      <TextField fx:id="groupName" layoutX="23.0" layoutY="58.0" />
      <Label layoutX="25.0" layoutY="34.0" text="Group Name" />
      <Button fx:id="adminAdd" layoutX="105.0" layoutY="169.0" mnemonicParsing="false" onAction="#buttonHandle" text="Add Admin" />
      <Button fx:id="finish" layoutX="211.0" layoutY="403.0" mnemonicParsing="false" onAction="#finish" text="Finish!" />
   </children>
</AnchorPane>
